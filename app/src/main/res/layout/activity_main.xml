<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.hm.iou.loginmodule.demo.MainActivity">

    <Button
        android:id="@+id/btn_open_launch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="打开启动页"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClickOpenLoginTypeSelect"
        android:text="打开LoginTypeSelect" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClickResetPsdBySMS"
        android:text="通过手机验证码重置登录密码" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClickBindEmail"
        android:text="绑定邮箱" />

</LinearLayout>
